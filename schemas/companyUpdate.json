{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "properties": {
      "handle": {
        "$id": "#/properties/handle",
        "type": "string",
        "title": "The Handle Schema",
        "default": "",
        "examples": [
          "SBUX"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Starbucks"
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/num_employees",
        "type": "integer",
        "title": "The Num_employees Schema",
        "default": 0,
        "examples": [
          500
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "coffee"
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logo_url",
        "type": "string",
        "title": "The Logo_url Schema",
        "default": "",
        "examples": [
          "https://images.unsplash.com/photo-1545231027-637d2f6210f8?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80"
        ],
        "pattern": "^(.*)$"
      }
    }
  }